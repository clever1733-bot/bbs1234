{
  "appId": "com.ptassessment.app",
  "appName": "PT Assessment",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#020617",
      "showSpinner": true,
      "spinnerColor": "#10B981"
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#020617"
    },
    "Camera": {
      "permissions": [
        "camera"
      ]
    }
  },
  "ios": {
    "contentInset": "automatic"
  },
  "android": {
    "allowMixedContent": true
  }
}
